<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wd.health">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application>
        <activity android:name="com.bw.health.activity.MainActivity">
        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bw.health.activity.LoginActivity" />
        <activity android:name="com.bw.health.activity.RegisterActivity" />
        <activity android:name="com.bw.health.activity.ForgetPasswordActivity" />
        <activity android:name="com.bw.health.activity.ForgetPassword2Activity"></activity>
    </application>

</manifest>
